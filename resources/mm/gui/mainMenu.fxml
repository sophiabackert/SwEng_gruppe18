<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mm.gui.GuiMainMenuController">
   <children>
      <VBox fx:id="vboxPane" alignment="CENTER" layoutX="250.0" layoutY="68.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Menu">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#startButtonOnAction" prefHeight="26.0" prefWidth="86.0" text="start game" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingsButtonOnAction" prefHeight="26.0" prefWidth="86.0" text="settings">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="tutorialButton" mnemonicParsing="false" onAction="#tutorialButtonOnAction" prefHeight="26.0" prefWidth="86.0" text="tutorial">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="aboutButton" mnemonicParsing="false" onAction="#aboutButtonOnAction" prefHeight="26.0" prefWidth="86.0" text="About">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="26.0" prefWidth="86.0" text="exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
