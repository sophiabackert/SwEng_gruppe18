<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mm.gui.SettingsController">
   <children>
      <VBox fx:id="vboxPane" alignment="CENTER" layoutX="213.0" layoutY="85.0" prefHeight="243.0" prefWidth="175.0">
         <children>
            <Label fx:id="musicLabel" alignment="CENTER" prefHeight="17.0" prefWidth="177.0" text="Music">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="volumeLabel" prefHeight="17.0" prefWidth="177.0" text="Volume" />
            <Slider fx:id="musikRegulator" accessibleText="Music" blockIncrement="1.0" prefHeight="26.0" prefWidth="128.0" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="25.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Slider>
            <CheckBox fx:id="muteCheckbox" mnemonicParsing="false" prefHeight="18.0" prefWidth="177.0" text="Mute" />
            <Button fx:id="backButton" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="26.0" prefWidth="50.0" text="back">
               <VBox.margin>
                  <Insets left="80.0" top="80.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
